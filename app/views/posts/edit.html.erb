
<h1> Edit post </h1>


<%= form_with model: @post do | form| %>
    <p>
    <%= form.label :title %>
    <%= form.text_field :title %>

    <%= form.label :description %>
    <%= form.text_area :description %>

    </p>
    <p>
    <%= form.submit %>
    </p>
    <%end %>
<%= link_to "Back", posts_path %>
